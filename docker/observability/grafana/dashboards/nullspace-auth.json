{
  "uid": "nullspace-auth",
  "title": "Nullspace Auth Service",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "annotations": {
    "list": [
      {
        "datasource": "Prometheus",
        "enable": true,
        "expr": "ALERTS{alertstate=\"firing\",job=\"auth\"}",
        "name": "Alerts",
        "step": "1m"
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "Overview",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Login Rate",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_auth_counter_total{key=\"auth.verify.success\"}[5m])",
          "legendFormat": "logins/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" }
            ]
          },
          "unit": "reqps"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 }
    },
    {
      "type": "stat",
      "title": "Failure Rate",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_auth_counter_total{key=\"auth.verify.failure\"}[5m])",
          "legendFormat": "failures/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 1, "color": "yellow" },
              { "value": 10, "color": "red" }
            ]
          },
          "unit": "reqps"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 }
    },
    {
      "type": "stat",
      "title": "Success Rate %",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_auth_counter_total{key=\"auth.verify.success\"}[5m]) / (rate(nullspace_auth_counter_total{key=\"auth.verify.success\"}[5m]) + rate(nullspace_auth_counter_total{key=\"auth.verify.failure\"}[5m])) * 100",
          "legendFormat": "success_rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "red" },
              { "value": 90, "color": "yellow" },
              { "value": 99, "color": "green" }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 }
    },
    {
      "type": "stat",
      "title": "Avg Latency",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "nullspace_auth_timing_avg_ms{key=\"http.request_ms\"}",
          "legendFormat": "avg_ms"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 100, "color": "yellow" },
              { "value": 500, "color": "red" }
            ]
          },
          "unit": "ms"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 }
    },
    {
      "type": "stat",
      "title": "Max Latency",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "nullspace_auth_timing_max_ms{key=\"http.request_ms\"}",
          "legendFormat": "max_ms"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 500, "color": "yellow" },
              { "value": 2000, "color": "red" }
            ]
          },
          "unit": "ms"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 }
    },
    {
      "type": "stat",
      "title": "Total Requests",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "nullspace_auth_timing_count_total{key=\"http.request_ms\"}",
          "legendFormat": "total"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "blue" }
            ]
          },
          "unit": "none"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 }
    },
    {
      "type": "row",
      "title": "RED Metrics - Rate",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 }
    },
    {
      "type": "timeseries",
      "title": "Authentication Rate (Rate)",
      "description": "Authentication attempts per second",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_auth_counter_total{key=\"auth.challenge.created\"}[5m])",
          "legendFormat": "challenge_created/sec"
        },
        {
          "expr": "rate(nullspace_auth_counter_total{key=\"auth.verify.success\"}[5m])",
          "legendFormat": "verify_success/sec"
        },
        {
          "expr": "rate(nullspace_auth_counter_total{key=\"auth.verify.failure\"}[5m])",
          "legendFormat": "verify_failure/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "verify_success/sec" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "verify_failure/sec" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 }
    },
    {
      "type": "timeseries",
      "title": "EVM Authentication Rate (Rate)",
      "description": "EVM wallet authentication attempts per second",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_auth_counter_total{key=\"auth.evm.created\"}[5m])",
          "legendFormat": "evm_created/sec"
        },
        {
          "expr": "rate(nullspace_auth_counter_total{key=\"auth.evm.success\"}[5m])",
          "legendFormat": "evm_success/sec"
        },
        {
          "expr": "rate(nullspace_auth_counter_total{key=\"auth.evm.failure\"}[5m])",
          "legendFormat": "evm_failure/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "evm_success/sec" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "evm_failure/sec" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 }
    },
    {
      "type": "row",
      "title": "RED Metrics - Errors",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 }
    },
    {
      "type": "timeseries",
      "title": "Auth Failures (Errors)",
      "description": "Authentication failures by type",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_auth_counter_total{key=\"auth.verify.failure\"}[5m])",
          "legendFormat": "verify_failure/sec"
        },
        {
          "expr": "rate(nullspace_auth_counter_total{key=\"auth.challenge.invalid\"}[5m])",
          "legendFormat": "challenge_invalid/sec"
        },
        {
          "expr": "rate(nullspace_auth_counter_total{key=\"auth.evm.invalid\"}[5m])",
          "legendFormat": "evm_invalid/sec"
        },
        {
          "expr": "rate(nullspace_auth_counter_total{key=\"auth.evm.failure\"}[5m])",
          "legendFormat": "evm_failure/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 }
    },
    {
      "type": "timeseries",
      "title": "Error Rate % (Errors)",
      "description": "Authentication error rate as percentage",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_auth_counter_total{key=\"auth.verify.failure\"}[5m]) / (rate(nullspace_auth_counter_total{key=\"auth.verify.success\"}[5m]) + rate(nullspace_auth_counter_total{key=\"auth.verify.failure\"}[5m])) * 100",
          "legendFormat": "error_rate_%"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "showPoints": "never"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 5, "color": "yellow" },
              { "value": 10, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 }
    },
    {
      "type": "row",
      "title": "RED Metrics - Duration",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 }
    },
    {
      "type": "timeseries",
      "title": "HTTP Request Latency (Duration)",
      "description": "HTTP request latency over time",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "nullspace_auth_timing_avg_ms{key=\"http.request_ms\"}",
          "legendFormat": "avg"
        },
        {
          "expr": "nullspace_auth_timing_max_ms{key=\"http.request_ms\"}",
          "legendFormat": "max"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 100, "color": "yellow" },
              { "value": 500, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 }
    },
    {
      "type": "timeseries",
      "title": "Auth Verify Latency (Duration)",
      "description": "Authentication verification latency",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "nullspace_auth_timing_avg_ms{key=\"auth.verify_ms\"}",
          "legendFormat": "verify_avg"
        },
        {
          "expr": "nullspace_auth_timing_max_ms{key=\"auth.verify_ms\"}",
          "legendFormat": "verify_max"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 }
    },
    {
      "type": "row",
      "title": "Freeroll Sync",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 32 }
    },
    {
      "type": "timeseries",
      "title": "Freeroll Sync Rate",
      "description": "Freeroll synchronization success and failure rates",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_auth_counter_total{key=\"freeroll.sync.success\"}[5m])",
          "legendFormat": "sync_success/sec"
        },
        {
          "expr": "rate(nullspace_auth_counter_total{key=\"freeroll.sync.failure\"}[5m])",
          "legendFormat": "sync_failure/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "sync_success/sec" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "sync_failure/sec" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 33 }
    },
    {
      "type": "timeseries",
      "title": "Freeroll Sync Latency",
      "description": "Freeroll sync latency metrics",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "nullspace_auth_timing_avg_ms{key=\"freeroll.sync_ms\"}",
          "legendFormat": "avg"
        },
        {
          "expr": "nullspace_auth_timing_max_ms{key=\"freeroll.sync_ms\"}",
          "legendFormat": "max"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 33 }
    },
    {
      "type": "row",
      "title": "Profile Operations",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 41 }
    },
    {
      "type": "timeseries",
      "title": "Profile Operations Rate",
      "description": "Profile fetch and update rates",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_auth_counter_total{key=\"auth.profile.fetch\"}[5m])",
          "legendFormat": "profile_fetch/sec"
        },
        {
          "expr": "rate(nullspace_auth_counter_total{key=\"auth.profile.update\"}[5m])",
          "legendFormat": "profile_update/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 42 }
    },
    {
      "type": "timeseries",
      "title": "Convex Mutation Latency",
      "description": "Convex backend mutation latency",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "nullspace_auth_timing_avg_ms{key=\"convex.mutation_ms\"}",
          "legendFormat": "avg"
        },
        {
          "expr": "nullspace_auth_timing_max_ms{key=\"convex.mutation_ms\"}",
          "legendFormat": "max"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 42 }
    }
  ]
}
