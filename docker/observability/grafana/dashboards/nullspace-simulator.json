{
  "uid": "nullspace-simulator",
  "title": "Nullspace Simulator",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "annotations": {
    "list": [
      {
        "datasource": "Prometheus",
        "enable": true,
        "expr": "ALERTS{alertstate=\"firing\",job=\"simulator\"}",
        "name": "Alerts",
        "step": "1m"
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "Overview",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 }
    },
    {
      "type": "stat",
      "title": "TPS (Transactions/sec)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_simulator_casino_games_started_total[5m]) + rate(nullspace_simulator_casino_games_moved_total[5m])",
          "legendFormat": "tps"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" }
            ]
          },
          "unit": "reqps"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 }
    },
    {
      "type": "stat",
      "title": "Submit p95 (ms)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(nullspace_simulator_http_submit_latency_ms_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 100, "color": "yellow" },
              { "value": 250, "color": "red" }
            ]
          },
          "unit": "ms"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 }
    },
    {
      "type": "stat",
      "title": "Error Rate",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_simulator_casino_errors_total[5m])",
          "legendFormat": "errors/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 1, "color": "yellow" },
              { "value": 10, "color": "red" }
            ]
          },
          "unit": "reqps"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 }
    },
    {
      "type": "stat",
      "title": "Active Sessions",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "nullspace_simulator_casino_active_sessions",
          "legendFormat": "sessions"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" }
            ]
          },
          "unit": "none"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 }
    },
    {
      "type": "stat",
      "title": "CPU Usage",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "nullspace_simulator_system_cpu_usage_percent",
          "legendFormat": "cpu"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 70, "color": "yellow" },
              { "value": 90, "color": "red" }
            ]
          },
          "unit": "percent"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 }
    },
    {
      "type": "stat",
      "title": "Memory (RSS)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "nullspace_simulator_system_rss_bytes",
          "legendFormat": "rss"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 1073741824, "color": "yellow" },
              { "value": 2147483648, "color": "red" }
            ]
          },
          "unit": "bytes"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 }
    },
    {
      "type": "row",
      "title": "RED Metrics - Rate",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 }
    },
    {
      "type": "timeseries",
      "title": "Transaction Rate (Rate)",
      "description": "Transactions submitted per second",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_simulator_casino_games_started_total[5m])",
          "legendFormat": "games_started/sec"
        },
        {
          "expr": "rate(nullspace_simulator_casino_games_moved_total[5m])",
          "legendFormat": "games_moved/sec"
        },
        {
          "expr": "rate(nullspace_simulator_casino_games_completed_total[5m])",
          "legendFormat": "games_completed/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 }
    },
    {
      "type": "timeseries",
      "title": "HTTP Request Rate (Rate)",
      "description": "HTTP API requests per second",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_simulator_http_submit_latency_ms_count[5m])",
          "legendFormat": "submit/sec"
        },
        {
          "expr": "rate(nullspace_simulator_http_query_state_latency_ms_count[5m])",
          "legendFormat": "query_state/sec"
        },
        {
          "expr": "rate(nullspace_simulator_http_query_seed_latency_ms_count[5m])",
          "legendFormat": "query_seed/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 }
    },
    {
      "type": "row",
      "title": "RED Metrics - Errors",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 }
    },
    {
      "type": "timeseries",
      "title": "Casino Errors (Errors)",
      "description": "Casino game errors per second",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_simulator_casino_errors_total[5m])",
          "legendFormat": "casino_errors/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "casino_errors/sec" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 }
    },
    {
      "type": "timeseries",
      "title": "HTTP Rejections (Errors)",
      "description": "HTTP request rejections by type",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_simulator_http_reject_origin_total[5m])",
          "legendFormat": "origin/sec"
        },
        {
          "expr": "rate(nullspace_simulator_http_reject_rate_limit_total[5m])",
          "legendFormat": "rate_limit/sec"
        },
        {
          "expr": "rate(nullspace_simulator_http_reject_body_limit_total[5m])",
          "legendFormat": "body_limit/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 }
    },
    {
      "type": "row",
      "title": "RED Metrics - Duration",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 }
    },
    {
      "type": "timeseries",
      "title": "Submit Latency (Duration)",
      "description": "Transaction submit latency percentiles",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(nullspace_simulator_http_submit_latency_ms_bucket[5m])) by (le))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(nullspace_simulator_http_submit_latency_ms_bucket[5m])) by (le))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(nullspace_simulator_http_submit_latency_ms_bucket[5m])) by (le))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 100, "color": "yellow" },
              { "value": 250, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 }
    },
    {
      "type": "timeseries",
      "title": "Query Latency (Duration)",
      "description": "State and seed query latency percentiles",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(nullspace_simulator_http_query_state_latency_ms_bucket[5m])) by (le))",
          "legendFormat": "query_state_p95"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(nullspace_simulator_http_query_seed_latency_ms_bucket[5m])) by (le))",
          "legendFormat": "query_seed_p95"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 }
    },
    {
      "type": "row",
      "title": "WebSocket",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 32 }
    },
    {
      "type": "timeseries",
      "title": "WebSocket Errors",
      "description": "WebSocket send errors and timeouts",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_simulator_ws_updates_send_errors_total[5m])",
          "legendFormat": "updates_send_errors/sec"
        },
        {
          "expr": "rate(nullspace_simulator_ws_mempool_send_errors_total[5m])",
          "legendFormat": "mempool_send_errors/sec"
        },
        {
          "expr": "rate(nullspace_simulator_ws_updates_send_timeouts_total[5m])",
          "legendFormat": "updates_timeouts/sec"
        },
        {
          "expr": "rate(nullspace_simulator_ws_mempool_send_timeouts_total[5m])",
          "legendFormat": "mempool_timeouts/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 33 }
    },
    {
      "type": "timeseries",
      "title": "WebSocket Connection Rejections",
      "description": "Connection rejections by limit type",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_simulator_ws_connection_reject_global_total[5m])",
          "legendFormat": "global/sec"
        },
        {
          "expr": "rate(nullspace_simulator_ws_connection_reject_per_ip_total[5m])",
          "legendFormat": "per_ip/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 33 }
    },
    {
      "type": "row",
      "title": "System Resources",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 41 }
    },
    {
      "type": "timeseries",
      "title": "CPU Usage",
      "description": "Process CPU usage percentage",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "nullspace_simulator_system_cpu_usage_percent",
          "legendFormat": "cpu_percent"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "showPoints": "never"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 70, "color": "yellow" },
              { "value": 90, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 42 }
    },
    {
      "type": "timeseries",
      "title": "Memory Usage",
      "description": "Process memory usage (RSS and Virtual)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "nullspace_simulator_system_rss_bytes / 1024 / 1024",
          "legendFormat": "rss_mb"
        },
        {
          "expr": "nullspace_simulator_system_virtual_bytes / 1024 / 1024",
          "legendFormat": "virtual_mb"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "decmbytes",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 42 }
    },
    {
      "type": "row",
      "title": "Update Index",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 50 }
    },
    {
      "type": "timeseries",
      "title": "Update Index Proofs",
      "description": "Proof generation latency and in-flight count",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(nullspace_simulator_update_index_proof_latency_ms_bucket[5m])) by (le))",
          "legendFormat": "proof_latency_p95"
        },
        {
          "expr": "nullspace_simulator_update_index_in_flight",
          "legendFormat": "in_flight"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 51 }
    },
    {
      "type": "timeseries",
      "title": "Update Index Failures",
      "description": "Update index failure rate",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_simulator_update_index_failures_total[5m])",
          "legendFormat": "failures/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "failures/sec" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 51 }
    }
  ]
}
