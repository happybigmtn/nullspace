{
  "uid": "nullspace-games",
  "title": "Nullspace Casino Games",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "annotations": {
    "list": [
      {
        "datasource": "Prometheus",
        "enable": true,
        "expr": "ALERTS{alertstate=\"firing\",alertname=~\".*casino.*\"}",
        "name": "Casino Alerts",
        "step": "1m"
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "Casino Overview",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Games Started (24h)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "increase(nullspace_simulator_casino_games_started_total[24h])",
          "legendFormat": "games"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "blue" }
            ]
          },
          "unit": "none",
          "decimals": 0
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 }
    },
    {
      "type": "stat",
      "title": "Games Completed (24h)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "increase(nullspace_simulator_casino_games_completed_total[24h])",
          "legendFormat": "games"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" }
            ]
          },
          "unit": "none",
          "decimals": 0
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 }
    },
    {
      "type": "stat",
      "title": "Active Sessions",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "nullspace_simulator_casino_active_sessions",
          "legendFormat": "active"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "purple" }
            ]
          },
          "unit": "none"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 }
    },
    {
      "type": "stat",
      "title": "Game Rate",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_simulator_casino_games_started_total[5m])",
          "legendFormat": "games/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" }
            ]
          },
          "unit": "reqps"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 }
    },
    {
      "type": "stat",
      "title": "Error Rate",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_simulator_casino_errors_total[5m])",
          "legendFormat": "errors/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 0.1, "color": "yellow" },
              { "value": 1, "color": "red" }
            ]
          },
          "unit": "reqps"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 }
    },
    {
      "type": "stat",
      "title": "Completion Rate %",
      "description": "Percentage of started games that complete successfully",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_simulator_casino_games_completed_total[5m]) / rate(nullspace_simulator_casino_games_started_total[5m]) * 100",
          "legendFormat": "completion_rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "red" },
              { "value": 90, "color": "yellow" },
              { "value": 99, "color": "green" }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 }
    },
    {
      "type": "row",
      "title": "RED Metrics - Rate (Bets/Games)",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 }
    },
    {
      "type": "timeseries",
      "title": "Game Lifecycle Rate (Rate)",
      "description": "Games started, moved, and completed per second",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_simulator_casino_games_started_total[5m])",
          "legendFormat": "started/sec"
        },
        {
          "expr": "rate(nullspace_simulator_casino_games_moved_total[5m])",
          "legendFormat": "moved/sec"
        },
        {
          "expr": "rate(nullspace_simulator_casino_games_completed_total[5m])",
          "legendFormat": "completed/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "started/sec" },
            "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "completed/sec" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "moved/sec" },
            "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 }
    },
    {
      "type": "timeseries",
      "title": "Active Sessions (Rate)",
      "description": "Current active game sessions over time",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "nullspace_simulator_casino_active_sessions",
          "legendFormat": "active_sessions"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 }
    },
    {
      "type": "row",
      "title": "RED Metrics - Errors",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 }
    },
    {
      "type": "timeseries",
      "title": "Casino Error Rate (Errors)",
      "description": "Game errors per second",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_simulator_casino_errors_total[5m])",
          "legendFormat": "errors/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "showPoints": "never"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 0.1, "color": "yellow" },
              { "value": 1, "color": "red" }
            ]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "errors/sec" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 }
    },
    {
      "type": "timeseries",
      "title": "Error Rate % (Errors)",
      "description": "Game error rate as percentage of total games",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_simulator_casino_errors_total[5m]) / (rate(nullspace_simulator_casino_games_started_total[5m]) + 0.001) * 100",
          "legendFormat": "error_rate_%"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "showPoints": "never"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 1, "color": "yellow" },
              { "value": 5, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 }
    },
    {
      "type": "row",
      "title": "RED Metrics - Duration",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 }
    },
    {
      "type": "timeseries",
      "title": "Submit Latency (Duration)",
      "description": "Game transaction submission latency",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(nullspace_simulator_http_submit_latency_ms_bucket[5m])) by (le))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(nullspace_simulator_http_submit_latency_ms_bucket[5m])) by (le))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(nullspace_simulator_http_submit_latency_ms_bucket[5m])) by (le))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 100, "color": "yellow" },
              { "value": 250, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 }
    },
    {
      "type": "timeseries",
      "title": "State Query Latency (Duration)",
      "description": "Game state query latency",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(nullspace_simulator_http_query_state_latency_ms_bucket[5m])) by (le))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(nullspace_simulator_http_query_state_latency_ms_bucket[5m])) by (le))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(nullspace_simulator_http_query_state_latency_ms_bucket[5m])) by (le))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 }
    },
    {
      "type": "row",
      "title": "Leaderboard & Tournaments",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 32 }
    },
    {
      "type": "timeseries",
      "title": "Leaderboard Updates",
      "description": "Leaderboard update rate",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_simulator_casino_leaderboard_updates_total[5m])",
          "legendFormat": "updates/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 33 }
    },
    {
      "type": "timeseries",
      "title": "Tournament Activity",
      "description": "Tournament starts and ends",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(nullspace_simulator_tournament_started_total[5m])",
          "legendFormat": "started/sec"
        },
        {
          "expr": "rate(nullspace_simulator_tournament_ended_total[5m])",
          "legendFormat": "ended/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "started/sec" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "ended/sec" },
            "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 33 }
    },
    {
      "type": "row",
      "title": "Cumulative Totals",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 41 }
    },
    {
      "type": "timeseries",
      "title": "Total Games (Cumulative)",
      "description": "Cumulative game counts over time",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "nullspace_simulator_casino_games_started_total",
          "legendFormat": "started"
        },
        {
          "expr": "nullspace_simulator_casino_games_completed_total",
          "legendFormat": "completed"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 42 }
    },
    {
      "type": "timeseries",
      "title": "Total Errors (Cumulative)",
      "description": "Cumulative error count over time",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "nullspace_simulator_casino_errors_total",
          "legendFormat": "errors"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "errors" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 42 }
    }
  ]
}
