{
  "name": "@nullspace/constants",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "react-native": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./games": {
      "types": "./dist/games.d.ts",
      "react-native": "./dist/games.js",
      "import": "./dist/games.js",
      "require": "./dist/games.js"
    },
    "./moves": {
      "types": "./dist/moves.d.ts",
      "react-native": "./dist/moves.js",
      "import": "./dist/moves.js",
      "require": "./dist/moves.js"
    },
    "./chips": {
      "types": "./dist/chips.d.ts",
      "react-native": "./dist/chips.js",
      "import": "./dist/chips.js",
      "require": "./dist/chips.js"
    },
    "./bet-types": {
      "types": "./dist/bet-types.d.ts",
      "react-native": "./dist/bet-types.js",
      "import": "./dist/bet-types.js",
      "require": "./dist/bet-types.js"
    },
    "./limits": {
      "types": "./dist/limits.d.ts",
      "react-native": "./dist/limits.js",
      "import": "./dist/limits.js",
      "require": "./dist/limits.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@nullspace/types": "workspace:*"
  },
  "devDependencies": {
    "@nullspace/tsconfig": "workspace:*",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  }
}
