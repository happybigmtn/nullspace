{
  "name": "nullspace-mobile",
  "version": "1.0.0",
  "main": "App.tsx",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "npx tsx tests/integration/runTests.ts",
    "test:integration:local": "GATEWAY_URL=ws://localhost:9010 npx tsx tests/integration/runTests.ts",
    "test:integration:staging": "GATEWAY_URL=wss://staging-api.nullspace.casino/ws npx tsx tests/integration/runTests.ts",
    "test:integration:production": "GATEWAY_URL=wss://api.nullspace.casino/ws npx tsx tests/integration/runTests.ts",
    "typecheck": "tsc --noEmit",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "e2e:ios": "npm run e2e:build:ios && npm run e2e:test:ios",
    "e2e:android": "npm run e2e:build:android && npm run e2e:test:android",
    "prebuild": "expo prebuild --clean"
  },
  "dependencies": {
    "@expo-google-fonts/outfit": "^0.4.3",
    "@expo-google-fonts/plus-jakarta-sans": "^0.4.2",
    "@noble/ciphers": "^0.6.0",
    "@noble/curves": "^1.7.0",
    "@noble/hashes": "^1.8.0",
    "@nullspace/constants": "workspace:*",
    "@nullspace/design-tokens": "workspace:*",
    "@nullspace/game-state": "workspace:*",
    "@nullspace/protocol": "workspace:*",
    "@nullspace/types": "workspace:*",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/native-stack": "^7.0.0",
    "@react-navigation/stack": "^7.6.13",
    "@sentry/react-native": "^7.8.0",
    "@shopify/react-native-skia": "2.2.12",
    "@types/ws": "^8.18.1",
    "expo": "~54.0.0",
    "expo-av": "^16.0.8",
    "expo-blur": "^15.0.8",
    "expo-constants": "~18.0.12",
    "expo-crypto": "~15.0.8",
    "expo-device": "~8.0.10",
    "expo-font": "^14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-local-authentication": "~17.0.8",
    "expo-notifications": "~0.32.15",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "^31.0.13",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-mmkv": "^3.2.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.2",
    "react-native-worklets": "0.5.1",
    "ws": "^8.18.3",
    "zod": "^4.2.1",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@config-plugins/detox": "^11.0.0",
    "@types/jest": "^29.5.0",
    "@types/react": "~19.1.17",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "babel-preset-expo": "^54.0.9",
    "detox": "^20.46.3",
    "eslint": "^8.57.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-plugin-expo": "^1.0.0",
    "jest": "^29.7.0",
    "jest-circus": "^30.2.0",
    "jest-expo": "~54.0.16",
    "react-test-renderer": "19.1.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.6.0"
  },
  "private": true
}
