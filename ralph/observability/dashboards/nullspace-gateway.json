{
  "uid": "nullspace-gateway",
  "title": "Nullspace Gateway",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "annotations": {
    "list": [
      {
        "datasource": "Prometheus",
        "enable": true,
        "expr": "ALERTS{alertstate=\"firing\",job=\"gateway\"}",
        "name": "Alerts",
        "step": "1m"
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "Overview",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Active Sessions",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "gateway_sessions_active",
          "legendFormat": "sessions"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 15000, "color": "yellow" },
              { "value": 19000, "color": "red" }
            ]
          },
          "unit": "none"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 }
    },
    {
      "type": "stat",
      "title": "Connection Rate",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(gateway_connections_connect_total[5m])",
          "legendFormat": "conn/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 100, "color": "yellow" },
              { "value": 500, "color": "red" }
            ]
          },
          "unit": "reqps"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 }
    },
    {
      "type": "stat",
      "title": "Message Rate",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(gateway_messages_total[5m]))",
          "legendFormat": "msg/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" }
            ]
          },
          "unit": "reqps"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 }
    },
    {
      "type": "stat",
      "title": "Error Rate",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(gateway_messages_error_total[5m])",
          "legendFormat": "err/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 1, "color": "yellow" },
              { "value": 10, "color": "red" }
            ]
          },
          "unit": "reqps"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 }
    },
    {
      "type": "stat",
      "title": "Rate Limit Hits",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(gateway_rate_limits_total[5m])",
          "legendFormat": "hits/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 1, "color": "yellow" },
              { "value": 10, "color": "red" }
            ]
          },
          "unit": "reqps"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 }
    },
    {
      "type": "stat",
      "title": "Uptime",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "gateway_uptime_seconds",
          "legendFormat": "uptime"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" }
            ]
          },
          "unit": "s"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 }
    },
    {
      "type": "row",
      "title": "RED Metrics - Rate",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 }
    },
    {
      "type": "timeseries",
      "title": "Connection Rate (Rate)",
      "description": "New WebSocket connections per second",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(gateway_connections_connect_total[5m])",
          "legendFormat": "connects/sec"
        },
        {
          "expr": "rate(gateway_connections_disconnect_total[5m])",
          "legendFormat": "disconnects/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 }
    },
    {
      "type": "timeseries",
      "title": "Message Rate by Type (Rate)",
      "description": "Messages processed per second by message type",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(gateway_messages_total[5m])",
          "legendFormat": "{{ type }}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 }
    },
    {
      "type": "row",
      "title": "RED Metrics - Errors",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 }
    },
    {
      "type": "timeseries",
      "title": "Error Rate (Errors)",
      "description": "Message handling errors per second",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(gateway_messages_error_total[5m])",
          "legendFormat": "errors/sec"
        },
        {
          "expr": "rate(gateway_messages_success_total[5m])",
          "legendFormat": "success/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "errors/sec" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "success/sec" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 }
    },
    {
      "type": "timeseries",
      "title": "Rate Limit Hits (Errors)",
      "description": "Rate limit enforcement by type",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(gateway_rate_limits_ip_limit_total[5m])",
          "legendFormat": "ip_limit/sec"
        },
        {
          "expr": "rate(gateway_rate_limits_session_cap_total[5m])",
          "legendFormat": "session_cap/sec"
        },
        {
          "expr": "rate(gateway_rate_limits_session_rate_limit_total[5m])",
          "legendFormat": "session_rate/sec"
        },
        {
          "expr": "rate(gateway_rate_limits_metrics_rate_limit_total[5m])",
          "legendFormat": "metrics_rate/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 }
    },
    {
      "type": "row",
      "title": "RED Metrics - Duration",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 }
    },
    {
      "type": "timeseries",
      "title": "Message Latency p95 (Duration)",
      "description": "95th percentile message handling latency",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(gateway_message_latency_ms_bucket[5m])) by (le))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(gateway_message_latency_ms_bucket[5m])) by (le))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 100, "color": "yellow" },
              { "value": 250, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 }
    },
    {
      "type": "timeseries",
      "title": "Message Latency Distribution (Duration)",
      "description": "Latency histogram buckets",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(gateway_message_latency_ms_bucket[5m])) by (le))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.90, sum(rate(gateway_message_latency_ms_bucket[5m])) by (le))",
          "legendFormat": "p90"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(gateway_message_latency_ms_bucket[5m])) by (le))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 }
    },
    {
      "type": "row",
      "title": "Sessions",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 32 }
    },
    {
      "type": "timeseries",
      "title": "Active Sessions",
      "description": "Current number of active WebSocket sessions",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "gateway_sessions_active",
          "legendFormat": "active"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "showPoints": "never"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 15000, "color": "yellow" },
              { "value": 19000, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 33 }
    },
    {
      "type": "timeseries",
      "title": "Session Lifecycle",
      "description": "Session creation and destruction rate",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(gateway_sessions_created_total[5m])",
          "legendFormat": "created/sec"
        },
        {
          "expr": "rate(gateway_sessions_destroyed_total[5m])",
          "legendFormat": "destroyed/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "created/sec" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "destroyed/sec" },
            "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 33 }
    },
    {
      "type": "row",
      "title": "Drain Status",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 41 }
    },
    {
      "type": "stat",
      "title": "Draining",
      "description": "Gateway drain status for graceful shutdown",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "gateway_draining",
          "legendFormat": "draining"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "Normal", "color": "green" } } },
            { "type": "value", "options": { "1": { "text": "Draining", "color": "yellow" } } }
          ]
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 42 }
    },
    {
      "type": "stat",
      "title": "Games in Progress",
      "description": "Active games during drain",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "gateway_drain_games_remaining",
          "legendFormat": "games"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 1, "color": "yellow" }
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 42 }
    }
  ]
}
